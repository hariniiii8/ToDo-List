
  <%- include("header")-%>
  <div class="box" id="heading">
    <h1> <%= listtitle %></h1>
  </div>
<div class="box">


  <%  for(var i=0;i<listitem.length;i++){%>
    <div class="item">
      <input type="checkbox" >
        <p><%=listitem[i] %></p>
    </div>

  <%}%>
  <form class="item" action="/" method="post">
    <input type="text"  name="listitem" placeholder="New Item" autocomplete="off">
    <button type="submit" placeholder="New Item" name="list" value=<%= listtitle %> >+</button>
  </form>


</div>
  <%- include("footer")-%>
